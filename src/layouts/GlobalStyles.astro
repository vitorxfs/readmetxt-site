<style is:global>
  :root {
    scroll-behavior: smooth;
  }

  *::-webkit-scrollbar-track {
    background-color: transparent;
  }

  *::-webkit-scrollbar {
    width: 6px;
    height: 6px;
    background-color: transparent;
  }

  ::-moz-selection {
    color: black;
    background: #C9A3FF;
  }

  ::selection {
    color: black;
    background: #C9A3FF;
  }
</style>
